{
  "name": "stevens_trader",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "compile": "babel --presets es2015,stage-2 -d compile/ src/",
    "test": "NODE_ENV=test mocha --timeout 20000 --compilers js:babel-core/register",
    "jest:test": "jest __tests__",
    "dev": "npm-run-all --parallel watch:server watch:build",
    "watch:build": "webpack --watch",
    "watch:server": "nodemon \"./src/server.js\" --watch \"./src\" --exec babel-node",
    "start": "babel-watch \"./src/server.js\"",
    "build-server": "webpack -p --config ./webpack.production.config.js && mkdir -p ../production && rm -rf ../production/src ../production/public && cp -R public package.json ../production && babel -d ../production/src ./src -s && touch ../production/Procfile && echo \"web: npm run production\" >> ../production/Procfile",
    "production": "node ./src/server.js"
  },
